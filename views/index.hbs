    <!-- Login -->
    <form class="bg-modal" method="POST" action="Login">
        <div class="modal-content">
            <div class="close">+</div>
            <img src="../public/img/img5.png" class="taille" alt="logo">
            <div class="form">
              <input type="Email" placeholder="Email" name="email" class="entrer" onkeyup='this.value=this.value.toUpperCase()'>
              <div class="erreur"></div>
              <input type="password" placeholder="Mot passe" name="mtp" class="entrer" onkeyup='this.value=this.value.toUpperCase()'>
              <div class="erreur"></div>
               <input class="button" class="btnconnecter" type="submit" id="btncss" class="enter2" value="Se connecter" >
            
        </div>
    </div>
    </form>
    <!-- Inscription -->

    <form class="bg-modal-2" method="POST" action="Inscription">
        <div class="modal-content2">
            <div class="close">+</div>
            <img src="../public/img/img5.png" class="taille" alt="logo">
            <div class="form">
              <input type="text" placeholder="name" class="enter2" name="nom" onkeyup='this.value=this.value.toUpperCase()'>
              <div class="erreur2"></div>
              <input type="Email" placeholder="Email" class="enter2" name="email" onkeyup='this.value=this.value.toUpperCase()'>
              <div class="erreur2"></div>
              <input type="password" placeholder="Mot passe" class="enter2" name="password" onkeyup='this.value=this.value.toUpperCase()'>
              <div class="erreur2"></div>
              <input type="password" placeholder="Confirmer Mot passe" class="enter2" onkeyup='this.value=this.value.toUpperCase()'>
              <div class="erreur2"></div>
              <input class="button" type="submit" id="btninscription" class="enter2" value="S'inscrire" >
            </div>
        </div>
    </form>


